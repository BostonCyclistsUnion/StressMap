{
    "version": 1,
    "layers": {
      "lts": {
        "source": "mapbox://tileset-source/skilcoyne/stressmap",
        "minzoom": 11,
        "maxzoom": 16,
        "features": {
            "id": [ "get", "osmid"],
            "attributes": {
                "set": {
                    "quality": [
                    "match",
                    [ "get", "highway" ],
                    "primary", 11,
                    "trunk", 11,
                    "motorway", 11,
                    "motorway_link", 11,
                    "cycleway", 11,
                    "secondary", 12,
                    "tertiary", 13,
                    "residential", 14,
                    "service", 15,
                    "footway", 16,
                    "pedestrian", 16,
                    16
                    ]
                }
                },
                "filter": [ "<=", [ "get", "quality" ], [ "zoom" ] ],
                "simplification": 10
            },
        "tiles": {
            "layer_size": 2500
        }
        }
    }
}